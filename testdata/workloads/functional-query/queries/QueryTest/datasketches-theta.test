    ds_theta_estimate(f) as f,
    ds_theta_estimate(d) as d,
    ds_theta_estimate(s) as s,
    ds_theta_estimate(c) as c,
    ds_theta_estimate(v) as v,
    ds_theta_estimate(nc) as nc
from theta_sketches_from_hive;
