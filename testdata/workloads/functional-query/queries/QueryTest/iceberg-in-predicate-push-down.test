# Test INT/BIGINT/FLOAT/DOUBLE/STRING/TIMESTAMP/DATE InPredicate push-down
create table ice_pred_pd1 (
  col_i INT,
  col_bi BIGINT,
  col_f FLOAT,
  col_db DOUBLE,
  col_str STRING,
  col_ts TIMESTAMP,
  col_dt DATE
) partitioned by spec (col_i) stored as iceberg tblproperties ('write.format.default' = 'parquet');
