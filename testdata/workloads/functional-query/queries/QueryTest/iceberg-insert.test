# Create table with decimal types
CREATE TABLE decimal_tbl (
  d1 DECIMAL(9,0),
  d2 DECIMAL(10,0),
  d3 DECIMAL(20,10),
  d4 DECIMAL(38,38),
  d5 DECIMAL(10,5),
