---- QUERY
# Uncorrelated subquery in arithmetic expr that returns scalar value at runtime.
SELECT count(id) FROM alltypes
WHERE int_col =
  3 * (SELECT int_col
       FROM alltypessmall
       WHERE id = 1)
---- RESULTS
730
---- TYPES
BIGINT
