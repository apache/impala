---- QUERY
# Uncorrelated subquery in binary predicate that returns scalar value at runtime.
# num_nodes = 1 is set for this file by the python test.
SELECT count(id) FROM alltypes
WHERE int_col =
  (SELECT int_col
   FROM alltypessmall
   WHERE id = 1)
---- RESULTS
730
---- TYPES
BIGINT
