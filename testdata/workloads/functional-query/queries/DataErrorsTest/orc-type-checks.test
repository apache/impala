  id int,
  u uniontype<int, boolean>,
  int_array array<int>,
  int_map map<string, int>)
stored as orc
tblproperties ('external.table.purge'='TRUE');
insert into $DATABASE.union_complextypes
  select 0, create_union(1, 50, false), array(0), map('key0', 0);
