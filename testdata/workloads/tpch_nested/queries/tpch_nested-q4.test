---- QUERY: TPCH-Q4
# Q4 - Order Priority Checking Query
select
  o_orderpriority,
  count(*) as order_count
from
  customer c,
  c.c_orders o
where
  o_orderdate >= '1993-07-01'
  and o_orderdate < '1993-10-01'
  and exists (
    select
      *
    from
      o.o_lineitems
    where
      l_commitdate < l_receiptdate
    )
group by
  o_orderpriority
order by
  o_orderpriority
