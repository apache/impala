---- QUERY: TPCH-Q18
# Q18 - Large Value Customer Query
select
  c_name,
  c_custkey,
  o_orderkey,
  o_orderdate,
  o_totalprice,
  sum_quantity
from
  customer c,
  c.c_orders o,
  (select sum(l_quantity) sum_quantity from o.o_lineitems) l
where
  sum_quantity > 300
order by
  o_totalprice desc,
  o_orderdate
limit 100
