  n_name,
  round(sum(l_extendedprice * (1 - l_discount)), 5) as revenue
from tpch.lineitem l
join tpch.orders o
  on (l_orderkey = o_orderkey)
join tpch.supplier s
  on (l_suppkey = s_suppkey)
