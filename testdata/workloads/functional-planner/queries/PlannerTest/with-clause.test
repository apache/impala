# Multiple dependent views in with-clause
with t1 as (
  select int_col c1, tinyint_col c2, max(id) c3
  from functional.alltypessmall
  group by 1, 2
  order by 1,2
  limit 5),
t2 as (select c1, c2, c3 from t1),
t3 as (
  select c1, c3, max(c2) m2
  from t2
  group by c1, c3
  limit 10),
t4 as (select c1, c3, m2 from t3)
select * from t4
where c1 > 0
order by c3, c1 desc
limit 3
