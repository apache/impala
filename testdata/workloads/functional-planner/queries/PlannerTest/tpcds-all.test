  GROUP BY
    ss_ticket_number,
    ss_customer_sk
  ) dj
JOIN customer c
  ON (dj.ss_customer_sk = c.c_customer_sk)
WHERE
 cnt BETWEEN 1 AND 5
ORDER BY
