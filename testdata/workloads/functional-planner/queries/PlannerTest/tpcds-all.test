# TPCDS-Q98
select
  i_item_desc,
  i_category,
  i_class,
  i_current_price,
  sum(ss_ext_sales_price) as itemrevenue
from
  store_sales
  join item on (store_sales.ss_item_sk = item.i_item_sk)
where
  i_category in('Jewelry', 'Sports', 'Books')
  and ss_date between '2001-01-12' and '2001-02-11'
group by
  i_item_desc,
  i_category,
  i_class,
  i_current_price
order by
  i_category,
  i_class,
  i_item_desc
limit 1000
