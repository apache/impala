# Q9 - Product Type Measure Query
select
  nation,
  o_year,
  round(sum(amount), 2) as sum_profit
from(
  select
    n_name as nation,
    year(o_orderdate) as o_year,
    round(l_extendedprice * (1 - l_discount) - ps_supplycost * l_quantity, 2) as amount
  from
    tpch_kudu.part,
    tpch_kudu.supplier,
    tpch_kudu.lineitem,
