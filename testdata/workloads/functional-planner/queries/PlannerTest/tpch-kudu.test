# Q2 - Minimum Cost Supplier Query
select
  round(s_acctbal, 2),
  s_name,
  n_name,
  p_partkey,
  p_mfgr,
  s_address,
  s_phone,
  s_comment
from
  tpch_kudu.part,
  tpch_kudu.supplier,
  tpch_kudu.partsupp,
  tpch_kudu.nation,
  tpch_kudu.region
where
  p_partkey = ps_partkey
  and s_suppkey = ps_suppkey
  and p_size = 15
  and p_type like '%BRASS'
  and s_nationkey = n_nationkey
  and n_regionkey = r_regionkey
  and r_name = 'EUROPE'
