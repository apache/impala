---- QUERY
# Large (more than 1 regular batches) distinct with multiple group by keys.
SELECT COUNT(*) FROM
(SELECT COUNT(DISTINCT p_partkey)
 FROM part GROUP BY p_brand, p_type, p_size) v
---- RESULTS
123039
---- TYPES
BIGINT
